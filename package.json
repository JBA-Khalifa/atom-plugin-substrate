{
  "name": "substrate-plugin",
  "main": "./lib/index",
  "version": "0.4.2",
  "description": "Plugin for Parity Substrate",
  "keywords": [
    "substrate",
    "parity",
    "polkadot",
    "web3",
    "everstake"
  ],
  "repository": "https://github.com/everstake/atom-plugin-substrate",
  "license": "Apache-2.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "atomTranspilers": [
    {
      "transpiler": "atom-ts-transpiler",
      "glob": "{!(node_modules)/**/,}*.ts?(x)",
      "options": {
        "compilerOptions": {},
        "cacheKeyFiles": [
          "tsconfig.json"
        ],
        "verbose": false
      }
    }
  ],
  "prettier": {},
  "dependencies": {
    "@polkadot/api": "^0.95.1",
    "@polkadot/api-contract": "^0.95.1",
    "@polkadot/keyring": "^1.5.1",
    "@polkadot/util-crypto": "^1.5.1",
    "atom-package-deps": "^5.1.0",
    "atom-ts-transpiler": "^1.5.2",
    "clipboardy": "^2.1.0",
    "list": "^2.0.14",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "typescript": "^3.0.3"
  },
  "devDependencies": {
    "@types/atom": "^1.36.1",
    "@types/electron": "^1.6.10",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-redux": "^7.1.4"
  }
}
